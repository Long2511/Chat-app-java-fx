<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<HBox spacing="10.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.ouroboros.chatapp.chatapp.Homepage.ChatListCellController">
    <children>
        <VBox spacing="3.0">
            <children>
                <Label fx:id="usernameLabel" style="-fx-font-weight: bold;" />
                <Label fx:id="lastMessageLabel" style="-fx-text-fill: gray;" />
            </children>
        </VBox>
        <Region HBox.hgrow="ALWAYS"/>
        <Label fx:id="timeLabel" style="-fx-text-fill: darkgray;" />
    </children>
</HBox>
